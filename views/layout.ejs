<script type="text/javascript">
  // For Notes Start
    $(document).ready(function () {
      var max_input = 25;
      var x = 1;
      $('.add-btn3').click(function (e) {
        e.preventDefault();
        if (x < max_input) { 
          x++;
          $('.wrapper3').append(`
          <div class="input-box">
            <div class="row">
            <div class="col-lg-10">
              <input type="text" name="notes" class="form-control mb-2" />
              </div>
              <a href="#" class="remove-lnk3" alt="Remove" title="Remove"><i class="fa fa-minus minus"></i></a>
              </div>
            </div>
          `);
        }
      });
      $('.wrapper').on("click", ".remove-lnk3", function (e) {
        e.preventDefault();
        $(this).parent('div').remove();
        x--;
      })
    });
    // For Notes Ends
</script>
